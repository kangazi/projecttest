<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="webproject.entity.member.Member">
	<insert id="mregister" parameterType="member">
		insert into  member_tb values(
						#{mb_businessno}, #{mb_name}, #{mb_pw},#{mb_ceo},#{mb_post},#{mb_addr1}, #{mb_addr2},
						#{mb_phone},#{mb_hp}, #{mb_type},'일반', sysdate)
	</insert>
	

	
</mapper>









